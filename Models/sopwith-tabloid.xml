<PropertyList>

 <path>sop-tabloid.ac</path>

  <effect>
     <inherits-from>Effects/model-transparent</inherits-from>
      <object-name>propdisk</object-name>
      <object-name>propblur</object-name>
    </effect>
 
 <!-- Control surfaces -->
 <!--<property>surface-positions/rudder-pos-norm</property>-->

 
  <model>
    <path>Aircraft/Sopwith_Tabloid/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -1.013 </x-m>
      <y-m> 0.0 </y-m>
      <z-m> -0.19 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg>  -85 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>


  <!-- ground dust -->
  <model>
    <path>Aircraft/Sopwith_Tabloid/Models/Effects/ground/ground0.xml</path>
    <offsets>
      <x-m> -1.042 </x-m>
      <y-m>  -0.93 </y-m>
      <z-m> -1.204 </z-m>
    </offsets>
  </model>

  <!-- ground dust -->
  <model>
    <path>Aircraft/Sopwith_Tabloid/Models/Effects/ground/ground1.xml</path>
    <offsets>
      <x-m> -1.042 </x-m>
      <y-m>  0.93 </y-m>
      <z-m> -1.204 </z-m>
    </offsets>
  </model>

  <!-- ground dust -->
  <model>
    <path>Aircraft/Sopwith_Tabloid/Models/Effects/ground/ground2.xml</path>   
    <offsets>
      <x-m> 3.0983 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.182 </z-m>
    </offsets>
  </model>

  <!-- pilot -->
  <model>
    <path>Aircraft/Sopwith_Tabloid/Models/Pilot/pilot.xml</path>   
    <offsets>
      <x-m> -0.213 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.012 </z-m>
    </offsets>
  </model>

<model>
    <path>Aircraft/Sopwith_Tabloid/Models/Effects/guns/fire.xml</path>
    <offsets>
      <x-m> -1.3838 </x-m>
      <y-m>  0.9687 </y-m>
      <z-m>  0.254 </z-m>
      <roll-deg> 0 </roll-deg>
      <pitch-deg> 0 </pitch-deg>
      <heading-deg> 149 </heading-deg>
    </offsets>
  </model>

   <!-- Control surfaces -->

  <animation>
  <type>rotate</type>
  <object-name>rudder</object-name>
  <property>surface-positions/rudder-pos-norm</property>-->
  <factor>26</factor>
  <center>
   <x-m>3.793</x-m>
   <z-m>0.582</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
     
 <animation>
  <type>rotate</type>
  <object-name>elevators</object-name>
  <property>surface-positions/elevator-pos-norm</property>
  <factor>17</factor>
  <center>
   <x-m>3.806</x-m>
   <z-m>0.358</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>leftaileron</object-name>
  <property>surface-positions/left-aileron-pos-norm</property>
  <factor>17</factor>
  <axis>
   <x1-m> -0.3229 </x1-m>
   <y1-m> -3.666 </y1-m>
   <z1-m> 1.288 </z1-m>
   <x2-m> -0.3235 </x2-m>
   <y2-m> -2.4971 </y2-m>
   <z2-m> 1.229 </z2-m>
  </axis>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>rightaileron</object-name>
  <property>surface-positions/right-aileron-pos-norm</property>
  <factor>-17</factor>
  <axis>
   <x1-m> -0.3229 </x1-m>
   <y1-m> 3.666 </y1-m>
   <z1-m> 1.288 </z1-m>
   <x2-m> -0.3235 </x2-m>
   <y2-m> 2.4971 </y2-m>
   <z2-m> 1.229 </z2-m>
  </axis>
 </animation>

   <!-- propeller selection with rpm spin -->

  <animation>
    <type>select</type>
    <object-name>propdisk</object-name>
    <condition>
      <greater-than>
        <property>engines/engine/rpm</property>
        <value>280</value>
      </greater-than>
    </condition>
  </animation>

  <!-- <animation>
    <type>select</type>
    <object-name>engineblur</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine/rpm</property>
        <value>5500.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine/rpm</property>
        <value>5000.001</value>
      </less-than>
    </and>
    </condition>
  </animation> -->

  <animation>
    <type>select</type>
    <object-name>RHprop</object-name>
    <condition>
      <less-than>
        <property>engines/engine/rpm</property>
	<value>300</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <name>PropComplete</name>
    <object-name>RHprop</object-name>
    <object-name>propmount</object-name>
    <object-name>propbol</object-name>
    <object-name>engine</object-name>
    <object-name>propdisk</object-name>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>PropComplete</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value> 15 </value>
      </greater-than>
    </condition>
    <property>engines/engine/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -2.023 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.00 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

   <!-- Gear Compression -->
  <animation>
    <name>Maingear</name>
    <object-name>leftwheel</object-name>
    <object-name>left-tire</object-name>
    <object-name>rightwheel</object-name>
    <object-name>right-tire</object-name>
    <object-name>axle</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Maingear</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.5 </ind><dep>  -2.15 </dep></entry>
      <entry><ind> 1.0 </ind><dep>   -3.94 </dep></entry>
    </interpolation>
    <center>
      <x-m>  -1.0798 </x-m>
      <y-m>  0.821 </y-m>
      <z-m> -1.2603 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>Maingear</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.5 </ind><dep>  -2.15 </dep></entry>
      <entry><ind> 1.0 </ind><dep>   -3.94 </dep></entry>
    </interpolation>
    <center>
      <x-m>  -1.0798 </x-m>
      <y-m>  -0.821 </y-m>
      <z-m> -1.2603 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

 <animation>
  <type>spin</type>
  <object-name>leftwheel</object-name>
  <object-name>left-tire</object-name>
  <condition>
     <property>gear/gear[0]/wow</property>
  </condition>
  <property>velocities/groundspeed-kt</property>
  <factor>15</factor> <!-- Wheel circumference 2.298 m -->
  <center>
   <x-m>-1.07986</x-m>
   <y-m>-0.9309</y-m>
   <z-m>-1.2636</z-m>
  </center>
  <axis>
   <x> 0.0</x>
   <y>-1.0</y>
   <z> 0.0</z>
  </axis>
  </animation>

 <animation>
  <type>spin</type>
  <object-name>rightwheel</object-name>
  <object-name>right-tire</object-name>
  <condition>
     <property>gear/gear[1]/wow</property>
  </condition>
  <property>velocities/groundspeed-kt</property>
  <factor>15</factor> <!-- Wheel circumference 2.298 m -->
  <center>
   <x-m>-1.07986</x-m>
   <y-m> 0.9309</y-m>
   <z-m>-1.2636</z-m>
  </center>
  <axis>
   <x> 0.0</x>
   <y>-1.0</y>
   <z> 0.0</z>
  </axis>
  </animation>

</PropertyList>